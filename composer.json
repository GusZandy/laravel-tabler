{
    "name": "guszandy/laravel-tabler",
    "description": "Laravel Package for integrating Tabler template and this package is Laravel Mix friendly.",
    "keywords": ["laravel", "tabler", "package", "composer"],
    "license": "MIT",
    "type": "package",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "^5.6 || 5.5.*"
    },
    "autoload": {
        "psr-4": {
            "GusZandy\\Tabler\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GusZandy\\Tabler\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GusZandy\\Tabler\\Providers\\AppServiceProvider"
            ]
        }
    }
}
