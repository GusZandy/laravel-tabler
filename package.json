        "jquery-circle-progress": "^1.2.2",
        "laravel-mix": "^2.1.11",
        "lodash": "^4.17.4",
        "popper.js": "^1.12",
        "requirejs": "^2.3.5",
        "sparkline": "^0.2.0",
        "tabler-ui": "0.0.32",
        "vue": "^2.5.7"
    }
